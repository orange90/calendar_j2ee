<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Check_login</servlet-name>
        <servlet-class>com.servlet.Check_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>com.servlet.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginSuccess</servlet-name>
        <servlet-class>com.servlet.LoginSuccess</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Setnewpassword</servlet-name>
        <servlet-class>com.servlet.Setnewpassword</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.servlet.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete_date</servlet-name>
        <servlet-class>com.servlet.delete_date</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Check_login</servlet-name>
        <url-pattern>/Check_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginSuccess</servlet-name>
        <url-pattern>/LoginSuccess</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Setnewpassword</servlet-name>
        <url-pattern>/Setnewpassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete_date</servlet-name>
        <url-pattern>/delete_date</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
